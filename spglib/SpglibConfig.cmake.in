# - Config file for the spglib package
# It defines the following variables
#  SPGLIB_INCLUDE_DIRS - include directories for FooBar
#  SPGLIB_LIBRARIES    - libraries to link against
 
# Compute paths
get_filename_component(SPGLIB_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
if(EXISTS "${SPGLIB_CMAKE_DIR}/CMakeCache.txt")
   # In build tree
   include("${SPGLIB_CMAKE_DIR}/SpglibBuildTreeSettings.cmake")
else()
   set(SPGLIB_INCLUDE_DIRS "${SPGLIB_CMAKE_DIR}/@CONF_REL_INCLUDE_DIR@")
endif()
 
# Our library dependencies (contains definitions for IMPORTED targets)
include("${SPGLIB_CMAKE_DIR}/SpglibDepends.cmake")
 
# These are IMPORTED targets created by SpglibDepends.cmake
set(SPGLIB_LIBRARIES spglib)